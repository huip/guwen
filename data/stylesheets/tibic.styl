
#main 
	margin: 50px 15% 0 15%
#registercontent
	width:220px
	height:34px
	border-radius:4px


.span12 
	width: 940px

.container-fluid 
	padding-right: 20px
	padding-left: 20px

div 
	display: block

.row-fluid
	width: 100%

.span10
	width: 900px

.register 
	margin:auto auto
	width: 550px
	height: 350px
	padding-top: 50px
	padding-bottom: 50px
	border-radius: 5px
	border-shadow: 0 1px 0 #f2f4f5
	border: 1px solid #ddd
	background: #fff

.form-control:focus 
	border-color: #66afe9
	outline: 0
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(102,175,233,0.6)
	box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(102,175,233,0.6)

.control-group 
	margin-bottom:5px
	margin-top:5px

.control-label 
	float: left
	width: 160px
	padding-top: 5px
	text-align: right

.controls 
	margin-left: 180px

.help-inline 
	display: inline-block
	padding-left: 5px
	vertical-align: middle

input:focus 
	border-color: rgba(82,168,236,0.8)
	outline: 0
	outline: thin dotted \9
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(82,168,236,0.6)
	-moz-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(82,168,236,0.6)
	box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(82,168,236,0.6)
